snippet date
	`system("date +'%Y-%m-%d %H:%M'")`
snippet \sum
	\sum\limits_{${1:}}^{${2:}} $0
snippet \prod
	\prod\limits_{${1:}}^{${2:}} $0
snippet \max
	\max\limits_{${1:}} $0
snippet \min
	\min\limits_{${1:}} $0
